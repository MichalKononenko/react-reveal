{
  "name": "react-reveal",
  "version": "0.0.1",
  "description": "A library for formatting reveal.js slide decks in React",
  "main": "index.js",
  "scripts": {
    "test": "node ./node_modules/karma/bin/karma start karma.conf.js",
    "continuous_test": "node ./node_modules/karma/bin/karma start --single-run=false",
    "compile": "node ./node_modules/webpack/bin/webpack.js",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.config.js --host 0.0.0.0",
    "dev_server": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "postinstall": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MichalKononenko/react-reveal.git"
  },
  "keywords": [
    "react",
    "reveal"
  ],
  "author": "Michal Kononenko",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MichalKononenko/react-reveal/issues"
  },
  "homepage": "https://github.com/MichalKononenko/react-reveal#readme",
  "dependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "coveralls": "^2.11.9",
    "esdoc": "^0.4.7",
    "expect": "^1.20.2",
    "file-loader": "^0.9.0",
    "istanbul": "^0.4.4",
    "istanbul-instrumenter-loader": "^0.2.0",
    "karma": "^1.1.1",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chrome-launcher": "^1.0.1",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.5.3",
    "phantomjs-prebuilt": "^2.1.7",
    "react": "^15.2.0",
    "react-dom": "^15.2.0"
  }
}
